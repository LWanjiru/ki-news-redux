{
  "name": "ki-news-redux",
  "version": "1.0.0",
  "description": "Consume NewsAPI with react-redux",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "open coverage/lcov-report/*.html",
    "test:coverage": "jest && npm run coverage",
    "lint": "eslint **.js",
    "start": "webpack-dev-server --mode development --open",
    "build": "webpack --mode production"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>__tests__/setup/setupEnzyme.js",
    "testPathIgnorePatterns": [
      "<rootDir>/__tests__/setup/"
    ],
    "collectCoverage": true,
    "coverageReporters": [
      "lcov"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LWanjiru/ki-news-redux.git"
  },
  "author": "Linnette Wanjiru",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LWanjiru/ki-news-redux/issues"
  },
  "homepage": "https://github.com/LWanjiru/ki-news-redux#readme",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-jest": "^23.0.1",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jest": "^21.17.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.8.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.1.0",
    "mini-css-extract-plugin": "^0.4.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "redux-mock-store": "^1.5.3",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.4",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "react-redux": "^5.0.7"
  },
  "env": {
    "jest/globals": true,
    "enzyme": true
  }
}
